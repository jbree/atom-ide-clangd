{
  "name": "ide-clangd",
  "main": "lib/main.js",
  "version": "0.4.0",
  "description": "C/C++ language support for Atom IDE",
  "repository": "https://github.com/jbree/ide-clangd",
  "keywords": [
    "atom-ide",
    "ide",
    "c",
    "c++",
    "cpp",
    "clang",
    "clangd",
    "languageserver",
    "language-server-protocol",
    "language-client"
  ],
  "author": "Joshua Breeden <jbreeden@me.com>",
  "license": "MIT",
  "engines ": {
    "atom": ">=1.21.0"
  },
  "dependencies": {
    "async": "^2.6.1",
    "atom-languageclient": "^0.9.7",
    "decompress": "^4.2.0",
    "decompress-tarxz": "^2.1.1",
    "hasbin": "^1.2.3",
    "htmlparser2": "^3.9.2",
    "node-fetch": "^2.2.0",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "enhancedScopes": [
    "source.c",
    "source.cpp"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "provideSignatureHelp"
      }
    }
  }
}
