{
  "name": "ide-clangd",
  "main": "lib/main.js",
  "version": "0.1.2",
  "description": "C/C++ language support for Atom IDE",
  "repository": "https://github.com/jbree/ide-clangd",
  "keywords": [
    "atom-ide",
    "ide",
    "c",
    "c++",
    "cpp",
    "clang",
    "clangd",
    "languageserver",
    "language-server-protocol",
    "language-client"
  ],
  "author": "Joshua Breeden <jbreeden@me.com>",
  "license": "MIT",
  "engines ": {
    "atom": ">=1.21.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.6.1",
    "decompress": "^4.2.0",
    "decompress-tarxz": "^2.1.1"
  },
  "enhancedScopes": [
    "source.c",
    "source.cpp"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    }
  }
}
